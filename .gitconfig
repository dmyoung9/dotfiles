[user]
    email = davidyoung@planetbids.com
    name = David Young

[core]
    pager = delta
    excludesFile = /home/davidyoung/.gitignore

[interactive]
    diffFilter = delta --color-only

[delta]
    navigate = true    # use n and N to move between diff sections
    light = false      # set to true if you're in a terminal w/ a light background color (e.g. the default macOS terminal)
    true-color = always
    line-numbers = true

[merge]
    conflictstyle = diff3

[diff]
    colorMoved = default
[alias]
    dog = log --decorate --oneline --graph --pretty=format:'%C(auto)%h%d %C(reset)%s %C(bold blue)%ce %C(reset)%C(green)%cr (%cd)'
